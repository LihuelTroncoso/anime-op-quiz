# YouTube data source
# Copy this file to .env and fill values.

# Required to read playlist items from YouTube Data API v3
YOUTUBE_API_KEY=
YOUTUBE_PLAYLIST_ID=

# Optional cache file path. Defaults to apps/api/data/openings.csv
YOUTUBE_CACHE_CSV=

# Single room password
ROOM_PASSWORD=

# Inactivity timeout before clearing all players
ROOM_IDLE_MINUTES=20

# Optional players score CSV. Defaults to apps/api/data/players-score.csv
PLAYERS_SCORE_CSV=
